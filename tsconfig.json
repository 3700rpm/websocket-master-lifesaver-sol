{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "resolveJsonModule": false,
    "noEmit": false,
    "skipDefaultLibCheck": true,
    "lib": ["DOM"],
  },
  "include": ["src"],
  "exclude": [
    "otherCode",
    "node_mudules",
    "dist"
  ]
}
