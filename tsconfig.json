{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "resolveJsonModule": false,
    "noEmit": false,
    "skipDefaultLibCheck": true,
    "lib": ["DOM"],
  },
  "exclude": [
    "otherCode"
  ]
}
